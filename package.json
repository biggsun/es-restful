{
  "name": "es-restful",
  "version": "1.2.2",
  "description": "easy & simple nodejs restful server framework",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc -p src",
    "test": "./node_modules/.bin/mocha -r ./node_modules/ts-node/register ./tests/*.ts",
    "cov": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- tests/*.ts --report lcovonly --compilers ts:ts-node/register && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "cover": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha -- tests/*.ts --report lcovonly --compilers ts:ts-node/register",
    "report": "mocha --recursive --reporter mochawesome",
    "preexample": "npm i",
    "example": "ts-node ./example/server.ts",
    "clean": "rimraf lib/*",    
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lleohao/restful.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "restful",
    "web",
    "framework"
  ],
  "author": "lleohao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lleohao/restful/issues"
  },
  "homepage": "https://github.com/lleohao/restful#readme",
  "devDependencies": {
    "@types/bluebird": "^3.0.37",
    "@types/mocha": "^2.2.33",
    "@types/node": "^6.0.51",
    "@types/should": "^8.1.30",
    "coveralls": "^2.11.15",
    "cz-conventional-changelog": "^1.2.0",
    "docsify": "^1.3.5",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mochawesome": "^1.5.4",
    "rimraf": "^2.6.1",
    "should": "^11.1.1",
    "ts-node": "^1.7.2",
    "typescript": "^2.0.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "bluebird": "^3.4.7"
  }
}
