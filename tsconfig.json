{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es6",
        "experimentalDecorators": true,

        "noImplicitAny": false,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "sourceMap": true
    },
    "exclude": [
        "node_modules",
        "typings"
    ]
}